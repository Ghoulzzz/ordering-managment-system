html
    head
        title Homepage
        script 
            include ../client.js
    body 
        div#main
        li
            a(href="/") Home
        li
            a(href="/users") Users
        li
            a(href="/register") Register
        br
        label(for="username") Username: 
        input(type="text" id="username" name="username")
        label(for="password")  Password:  
        input(type="password" id="password" name="password")
        button(type="button" onclick="loginCheck()") Login
        br
        h1 Profile for #{profilename} 
        h2 Order history:
        each key in orders 
            a(href=`/orders/${key}`) #{key} 
            br
